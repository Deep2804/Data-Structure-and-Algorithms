   "source": [
    "import random\n",
    "r=random.randint(1,20)\n",
    "while(True):\n",
    "    inp=int(input())\n",
    "    if(inp<r):\n",
    "        print(\"Wrong guess! try a greater number\")\n",
    "    elif(inp>r):\n",
    "        print(\"Wrong guess! try a smaller number\")\n",
    "    else:\n",
    "        print(\"Congrats! you've guess the right number\")\n",
    "        break;"
   ]
